{
  "name": "InstaClone",
  "reason": {
    "react-jsx": 3
  },
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "bs-dependencies": [
    "reason-apollo",
    "reason-apollo-hooks",
    "reason-react",
    "reason-react-native",
    "reason-expo",
    "@reason-react-native/navigation",
  ],
  "ppx-flags": [
    "graphql_ppx/ppx"
  ],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "suffix": ".bs.js",
  "refmt": 3,
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "shims": {},
    "debug": {
      "all": false,
      "basic": true
    }
  }
}
